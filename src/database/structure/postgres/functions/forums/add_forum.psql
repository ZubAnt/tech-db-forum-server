CREATE OR REPLACE FUNCTION add_forum(
  p_slug CITEXT,
  p_user_id INT,
  p_nickname CITEXT,
  p_title TEXT)

RETURNS void
AS
$BODY$
  BEGIN
    RETURN QUERY
        INSERT INTO forums (slug, user_id, nickname, title)
        VALUES (p_slug, p_user_id, p_nickname, p_title);

END;
$BODY$
LANGUAGE plpgsql VOLATILE;